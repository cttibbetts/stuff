! --------------------------
! ROFI color theme
! --------------------------
rofi.font: Roboto 18
rofi.lines: 13
rofi.width: 100
rofi.padding: 400
rofi.color-enabled: true
! state:           'background'   'border'      'seperator'
rofi.color-window: argb:f0273238, argb:f0273238, argb:00000000
! state:           'bg'           'fg'           'bgalt'        'hlbg'         'hlfg'
rofi.color-normal: argb:00000000, argb:ffc1c1c1, argb:00000000, argb:ff394249, argb:ffffffff
rofi.color-active: argb:00000000, argb:ff80cbc4, argb:00000000, argb:ff394249, argb:ff80cbc4
rofi.color-urgent: argb:00000000, argb:ffff1844, argb:00000000, argb:ff394249, argb:ffff1844
