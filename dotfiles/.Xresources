URxvt.font: xft:Roboto Mono for Powerline:pixelsize=13
URxvt.scrollBar: false

URxvt.depth: 32

! special
URxvt.foreground:   #d9d9d9
URxvt.background:   [85]#212121
URxvt.cursorColor:  #d9d9d9

! black
URxvt.color0:       #212121
URxvt.color8:       #616161

! red
URxvt.color1:       #d1392e
URxvt.color9:       #e91e63

! green
URxvt.color2:       #4caf50
URxvt.color10:      #8bc34a

! yellow
URxvt.color3:       #ff9800
URxvt.color11:      #ffc107

! blue
URxvt.color4:       #1f77e1
URxvt.color12:      #03a9f4

! magenta
URxvt.color5:       #6f3cca
URxvt.color13:      #9c27b0

! cyan
URxvt.color6:       #009688
URxvt.color14:      #4db6ac

! white
URxvt.color7:       #efefef
